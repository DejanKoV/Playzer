<Window x:Class="Playzer.LoadCSV"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Playzer"
        mc:Ignorable="d"
        Title="" Height="450" Width="800">
    <Grid>
        <DataGrid Name="dg" HorizontalAlignment="Left" AutoGenerateColumns="False" Height="419" VerticalAlignment="Top" Width="792">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Track ID" Width="*" Binding="{Binding Track_ID}"/>
                <DataGridTextColumn Header="Track Name" Width="*" Binding="{Binding Track_Name}"/>
                <DataGridTextColumn Header="Track Artist" Width="*" Binding="{Binding Track_Artist}"/>
                <DataGridTextColumn Header="Album Name" Width="*" Binding="{Binding Album_Name}"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
